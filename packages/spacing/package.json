{
  "name": "@butter-cream/spacing",
  "version": "1.0.0-alpha.1",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npm run bundle && cp dist/index.d.ts dist/index.d.cts",
    "bundle": "tsup src/index.ts --format esm,cjs --dts --minify terser --treeshake",
    "test": "vitest"
  },
  "dependencies": {
    "@muffin-tin/core": "1.0.0-alpha.3",
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/css-utils": "0.1.4",
    "@vanilla-extract/sprinkles": "^1.5.2",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.12",
    "@vanilla-extract/vite-plugin": "^4.0.16",
    "terser": "5.16.8",
    "tsup": "6.7.0",
    "typescript": "5.2.2",
    "vitest": "2.1.8"
  }
}
