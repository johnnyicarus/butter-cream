{
  "name": "@butter-cream/switcher",
  "version": "1.0.0-alpha.1",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npm run bundle && cp dist/index.d.ts dist/index.d.cts",
    "bundle": "tsup src/index.ts --format esm,cjs --dts --minify terser --treeshake"
  },
  "dependencies": {
    "@muffin-tin/core": "1.0.0-alpha.3",
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/sprinkles": "^1.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "^18",
    "@types/react-dom": "^18"
  },
  "devDependencies": {
    "terser": "5.16.8",
    "tsup": "6.7.0",
    "typescript": "5.2.2"
  }
}
